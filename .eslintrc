{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier"
  ],
  "globals": {
    "$": true,
    "browser": true,
    "device": true,
    "expect": true
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-use-before-define": 0,
    "react/jsx-one-expression-per-line": 0,
    "prettier/prettier": "error"
  }
}
